{
  "name": "template-mecaota-frontend",
  "version": "0.0.1",
  "dependencies": {
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
    "framer-motion": "5.6.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.3",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.1",
    "storybook-builder-vite": "^0.1.10",
    "ts-jest": "^27.0.7",
    "vite": "^2.6.14",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "lint-staged": {
    "*.{json,js,ts,tsx}": [
      "prettier --ignore-unknown --no-error-on-unmatched-pattern --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --ext .ts,.tsx --fix"
    ]
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --ignore-unknown --no-error-on-unmatched-pattern .",
    "prettier:fmt": "prettier --write .",
    "serve": "vite preview",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
