{
  "name": "template-mecaota-frontend",
  "version": "0.2.0",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@emotion/react": "^11.10.0",
    "@linaria/babel-preset": "^4.1.2",
    "@linaria/core": "^4.1.1",
    "@linaria/react": "^4.1.2",
    "@linaria/shaker": "^4.1.2",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "vite": "^3.0.4",
    "vite-plugin-linaria": "^1.0.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "lint-staged": {
    "*.{json,js,ts,tsx}": [
      "prettier --ignore-unknown --no-error-on-unmatched-pattern --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --ext .ts,.tsx --fix"
    ]
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint:check": "eslint . --ext .ts,.tsx",
    "lint:fmt": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --ignore-unknown --no-error-on-unmatched-pattern --ignore-path .gitignore .",
    "prettier:fmt": "prettier --ignore-unknown --no-error-on-unmatched-pattern --ignore-path .gitignore --write .",
    "serve": "vite preview",
    "storybook:build": "build-storybook",
    "storybook:dev": "start-storybook -p 6006",
    "test": "jest --passWithNoTests"
  }
}
