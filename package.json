{
  "name": "template-mecaota-frontend",
  "version": "0.1.0",
  "dependencies": {
    "@linaria/babel-preset": "^3.0.0-beta.22",
    "@linaria/core": "^3.0.0-beta.15",
    "@linaria/react": "^3.0.0-beta.15",
    "@linaria/shaker": "^3.0.0-beta.15",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "storybook-builder-vite": "^0.1.23",
    "ts-jest": "^28.0.7",
    "vite": "^2.9.9",
    "vite-plugin-linaria": "^1.0.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "lint-staged": {
    "*.{json,js,ts,tsx}": [
      "prettier --ignore-unknown --no-error-on-unmatched-pattern --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --ext .ts,.tsx --fix"
    ]
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "eslint:fmt": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --ignore-unknown --no-error-on-unmatched-pattern --ignore-path .gitignore .",
    "prettier:fmt": "prettier --ignore-unknown --no-error-on-unmatched-pattern --ignore-path .gitignore --write .",
    "serve": "vite preview",
    "storybook:build": "build-storybook",
    "storybook:dev": "start-storybook -p 6006",
    "test": "jest --passWithNoTests"
  }
}
